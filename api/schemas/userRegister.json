{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "User Schema",
  "required": [
    "username",
    "password",
    "email",
    "firstName",
    "lastName",
    "age",
    "gender"
  ],
  "properties": {
    "username": {
      "type": "string",
      "title": "The username Schema",
      "examples": ["Joey50"]
    },
    "password": {
      "type": "string",
      "title": "The password Schema",
      "examples": ["SandWich50"]
    },
    "email": {
      "type": "string",
      "title": "The email Schema",
      "format": "email",
      "examples": ["JoeyT@gmail.com"]
    },
    "firstName": {
      "type": "string",
      "title": "The firstName Schema",
      "examples": ["Joey"]
    },
    "lastName": {
      "type": "string",
      "title": "The lastName Schema",
      "examples": ["Tribiani"]
    },
    "age": {
      "type": "integer",
      "minimum": 4,
      "maximum": 110,
      "title": "The age Schema",
      "examples": [29]
    },
    "gender": {
      "type": "string",
      "title": "The gender Schema",
      "examples": ["male"]
    }
  },
  "examples": [
    {
      "username": "Joey50",
      "password": "SandWich50",
      "email": "JoeyT@gmail.com",
      "firstName": "Joey",
      "lastName": "Tribiani",
      "age": 29,
      "gender": "male"
    }
  ],
  "additionalProperties": false
}
